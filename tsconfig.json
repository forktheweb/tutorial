{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["esnext"],
    "strict": true,
    "rootDir": ".",
    "plugins": [{ "name": "nexus/typescript-language-service" }],
    "esModuleInterop": true,
    "typeRoots": ["node_modules/@types", "types"],
    "noEmit": true
  },
  "include": [".", "types.d.ts"]
}
